{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"@components/*": ["src/components/*"],
			"@styles/*": ["src/styles/*"],
			"@assets/*": ["assets/*"],
			"@utils/*": ["src/utils/*"],
			"@screens/*": ["src/screens/*"]
		},
		"baseUrl": "."
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
